{
  "name": "sre.app",
  "displayName": "sre.app",
  "version": "1.0.0",
  "description": "My personal app.",
  "main": "index.js",
  "scripts": {
    "init": "mkdir www && node_modules/cordova/bin/cordova prepare && npm run build",
    "build-ci": "mkdir www && node_modules/cordova/bin/cordova prepare && target=prod node_modules/rollup/bin/rollup -c && node_modules/cordova/bin/cordova build android browser",
    "build": "node_modules/rollup/bin/rollup -c && node_modules/cordova/bin/cordova build",
    "compile": "node_modules/rollup/bin/rollup -c",
    "watch": "npm-run-all --parallel watch-www watch-src",
    "watch-www": "./cordova-watch-build",
    "watch-src": "node_modules/rollup/bin/rollup -c -w",
    "dev": "target=dev npm-run-all --parallel start watch",
    "mock": "npm-run-all --parallel start watch",
    "start": "node_modules/cordova/bin/cordova run browser",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "./node_modules/.bin/eslint src/"
  },
  "author": "srenault.contact@gmail.com",
  "license": "Apache-2.0",
  "dependencies": {
    "@material-ui/core": "^4.0.2",
    "@material-ui/icons": "^4.0.1",
    "c3": "^0.7.1",
    "cordova-android": "^8.0.0",
    "cordova-browser": "6.0.0",
    "cordova-osx": "^5.0.0",
    "cordova-plugin-whitelist": "^1.3.3",
    "date-fns": "^1.30.1",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-async": "^7.0.0",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "react-router-prop-types": "^1.0.4",
    "rxjs": "^6.5.2"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "browser",
      "android",
      "osx"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.5",
    "chokidar": "^3.0.1",
    "cordova": "^9.0.0",
    "eslint": "^6.0.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "json-to-js": "^1.0.2",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.6.0",
    "rollup": "^1.14.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-copy": "^3.0.0",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-size": "0.0.1",
    "rollup-plugin-terser": "^5.0.0"
  }
}